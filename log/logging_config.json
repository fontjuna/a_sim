{
    "version": 1,
    "formatters": {
        "detailed": {
            "format": "%(asctime)s.%(msecs)03d-%(levelname)s-[%(filename)s(%(lineno)d) / %(funcName)s] %(message)s",
            "datefmt": "%Y-%m-%d %H:%M:%S"
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "formatter": "detailed",
            "level": "DEBUG"
        },
        "file": {
            "class": "logging.handlers.RotatingFileHandler",
            "filename": "log_yyyymmdd_00.log",
            "formatter": "detailed",
            "maxBytes": 1048576,
            "backupCount": 10,
            "encoding": "utf-8",
            "level": "DEBUG"
        }
    },
    "root": {
        "handlers": [
            "console",
            "file"
        ],
        "level": "DEBUG"
    }
}